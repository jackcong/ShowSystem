    <add key="ida:TenantId" value="0540929d-9b0a-4fed-bf97-fa6f9d4f5bdc"/>
    <add key="ida:PostLogoutRedirectUri" value="http://localhost:44328/"/>
    <add key="EnvType" value="Test"/>
    <add key="DownLoadVirtualPath" value="~\DownLoadData\"/>

    

  </appSettings>
